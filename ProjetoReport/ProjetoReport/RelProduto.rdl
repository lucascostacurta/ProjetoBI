  <DataSets>
    <DataSet Name="DataSetProdutos">
      <Query>
        <DataSourceName>DataSourceProdutos</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fornecedor">
            <Value>=Parameters!fornecedor.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct p.nome as produto, c.nome as categoria
from dim_produto p
inner join fato fa
on p.idsk = fa. idproduto
inner join dim_fornecedor f
on f.idsk = fa.idfornecedor
inner join categoria c
on idcategoria = id_categoria
where f.nome = @fornecedor</CommandText>
      </Query>
      <Fields>
        <Field Name="produto">
          <DataField>produto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="categoria">
          <DataField>categoria</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
