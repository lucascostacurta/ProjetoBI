                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT        c.idcliente, v.idvendedor, p.idproduto, fo.idfornecedor, n.idnota, f.idforma, i.quantidade, i.quantidade * p.custo_medio AS custo_total, i.total - i.quantidade * p.custo_medio AS lucro_total, i.total AS total_item, n.data
FROM            dbo.nota_fiscal AS n INNER JOIN
                         dbo.item_nota AS i ON n.idnota = i.id_nota_fiscal INNER JOIN
                         dbo.cliente AS c ON c.idcliente = n.id_cliente INNER JOIN
                         dbo.vendedor AS v ON v.idvendedor = n.id_vendedor INNER JOIN
                         dbo.produto AS p ON p.idproduto = i.id_produto INNER JOIN
                         dbo.forma_pagamento AS f ON f.idforma = n.id_forma INNER JOIN
                         dbo.fornecedor AS fo ON fo.idfornecedor = p.id_fornecedor</property>
